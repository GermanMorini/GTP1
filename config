#!/bin/bash

alias cls=clear

function cmpExe() {
        if [ ! -d "./exec" ]; then
                mkdir "./exec"
        fi

	g++ -o "./exec/${1%.*}" $* && "./exec/${1%.*}"
}

git config user.name "Programaci√≥n I"
git config user.email "2302130@ucc.edu.ar"
git config alias.logex "log --graph --abbrev-commit"
